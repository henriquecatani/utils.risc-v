{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Debug RISC-V",
            "type": "gdb",
            "request": "attach",
            "executable": "${workspaceRoot}/program",
            "remote": true,
            "target": ":1234",
            "gdbpath": "/usr/bin/riscv64-linux-gnu-gdb", // WORKS ON ARCH
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "start-qemu",
            "autorun": [
                "continue"
            ]
        }
    ]
}